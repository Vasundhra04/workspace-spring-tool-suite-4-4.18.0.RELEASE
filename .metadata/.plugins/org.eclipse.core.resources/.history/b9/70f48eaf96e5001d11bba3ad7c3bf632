package com.example.demo.dao;




import java.util.List;

import org.springframework.data.jpa.repository.JpaRepository;

import com.example.demo.model.Insta;


public interface InstaRepository extends JpaRepository<Insta,Integer> {

	List<Insta> findByNameStartingWith(String  prefix);
	List<Insta> findByNameEndingingWith(String  suffix);
	List<Insta> findByNameDepartment(String username);
}
